//Set these fields
var sourceTable = "alm_hardware";
var fieldName = "model_category";
var oldValue = "926bc5846faa450061b29b9eae3ee419";
var newValue = "dc69a7d4db15c6502d626fccd3961905";


var recordList = new GlideRecord(sourceTable);
recordList.addQuery(fieldName, "=", oldValue);
recordList.query();
while(recordList.next()){
  recordList.setValue(fieldName, newValue);
  recordList.update();
}
